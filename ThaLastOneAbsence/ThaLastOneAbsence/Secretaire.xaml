        <DataGrid x:Name="dg_ABS" HorizontalAlignment="Left" Height="427" Margin="252,170,0,-25.188" VerticalAlignment="Top" Width="711"  Opacity="0.7" Foreground="Black" Grid.ColumnSpan="2" Background="White" >


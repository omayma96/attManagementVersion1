        </DataGrid>
        <DataGrid x:Name="dg_FEBE" HorizontalAlignment="Left" Height="327" Margin="226,185,0,0" VerticalAlignment="Top" Width="758"  Opacity="0.7" Visibility="Hidden" Grid.ColumnSpan="2">

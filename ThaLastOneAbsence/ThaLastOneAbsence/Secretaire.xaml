        <DataGrid x:Name="dg_Dash" HorizontalAlignment="Left" Height="217" Margin="226,290,0,0" VerticalAlignment="Top" Width="758"  Opacity="0.7" Visibility="Hidden" Grid.ColumnSpan="2">

